import { getI18n } from '../i18n/'
const { currentLocale } = Astro
const i18n = getI18n({ currentLocale })
---

<Layout title={i18n.SEO_TITLE}
		description=`Somos un socio estratégico comprometido con revolucionar la manera en que brindamos servicios tecnológicos a nuestros aliados corporativos, comerciales y residenciales, estableciendo un progreso	continuo con excelentes resultados.`
>
	<Portada />

	<main>
		<Intro/>
